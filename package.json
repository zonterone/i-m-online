{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "i-m-online",
  "title": "I'm Online?",
  "description": "Checks internet availability",
  "icon": "command-icon.png",
  "author": "zonterone",
  "categories": [
    "System"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "menubar",
      "title": "I'm Online?",
      "description": "Checks internet availability",
      "mode": "menu-bar",
      "interval": "10s"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.85.2",
    "@raycast/utils": "^1.18.0"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^1.0.11",
    "@types/node": "20.17.6",
    "@types/react": "18.3.12",
    "eslint": "^8.57.1",
    "prettier": "^3.3.3",
    "typescript": "^5.6.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
